# usage of dictionary


#1st explain
dic={"a":56,"d":56,74:"f"}
for key,value in dic.items():
    print(value)




# 2st explain

print("a")
print(dic[45])




#3st explain

dic={"a":56,"d":56,74:"f"}
for key,value in dic.items():
    print(value)


#4st explain

for i in dic:
    print(i)




#5st explain

dic={"a":56,"d":56,74:"f"}

print(dic["a"],dic["d"],dic[74])










# Using of if elif else

a=int(input("Enter: "))
b=int(input("Enter: "))
opr=str(input("Enter: "))
if("+"==opr):
    print(a+b)
elif("-"==opr):
    print(a-b)
elif("*"==opr):
    print(a*b)
elif("/"==opr):
    print(a/b) 
else:
    print("default")
  



"""
